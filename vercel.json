{
    "builds": [
        {
            "src": "/client/build/*",
            "use": "@vercel/static-build"
        },
        {
            "src": "/server/*",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)", "dest": "/client/build/$1"
        }]
}